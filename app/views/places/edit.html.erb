<div class="container my-4">
  <h3>場所編集</h3>
  <div class="col-ld-6">
    <%= form_with model:@place, url: place_path(@place.id), local:true do |f| %>
      <div class="form-group">
        <%= f.label:場所名 %>
        <%= f.text_field :name %>
        <%= f.submit '変更を保存', class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>